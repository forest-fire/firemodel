    "firebase-key": "^2.0.2",
    "get-value": "^3.0.1",
    "lodash.first": "^3.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "set-value": "^3.0.0",
    "typed-conversions": "^0.10.2",
    "universal-fire": "^0.43.0",
    "wait-in-parallel": "^1.2.6"
  },
  "devDependencies": {
    "@firebase/auth-types": "^0.9.5",
    "@forest-fire/types": "^0.42.0",
    "@types/chai": "^4.2.9",
    "@types/faker": "^4.1.11",
    "@types/get-value": "^3.0.1",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@types/lodash.first": "^3.0.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^10.14.0",

{
  "name": "firemodel",
  "version": "0.14.0",
  "description": "Modeling, Access, and Mocking for Firebase Projects",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/firemodel",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/firemodel.cjs.js",
  "module": "dist/firemodel.es2015.js",
  "browser": "dist/firemodel.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "./node_modules/.bin/tslint --force --format verbose \"src/**/*.ts\"",
    "build": "tslint src/**/*.ts && rollup -c rollup.config.js",
    "test": "./node_modules/.bin/ts-node ./scripts/test.ts ",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "./node_modules/.bin/ts-node ./scripts/watch.ts",
    "watch:test": "./node_modules/.bin/ts-node ./scripts/watch.ts test",
    "watch:all": "./node_modules/.bin/ts-node ./scripts/watch.ts all",
    "deploy": "./node_modules/.bin/ts-node ./scripts/deploy.ts",
    "package": "./node_modules/.bin/ts-node ./scripts/package.ts ",
    "cli": "./node_modules/.bin/ts-node cli/index.ts --",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "common-types": "^1.8.2",
    "firebase-api-surface": "^0.8.3",
    "firebase-key": "^2.0.1",
    "lodash": "^4.17.10",
    "pluralize": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "serialized-query": "^0.4.4",
    "typed-conversions": "^0.8.9",
    "wait-in-parallel": "^1.1.4"
  },
  "peerDependencies": {
    "firemock": "^0.18.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chance": "^0.7.36",
    "@types/faker": "^4.1.0",
    "@types/js-yaml": "^3.9.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.2",
    "@types/node": "^9.6.2",
    "@types/pluralize": "^0.0.28",
    "@types/rimraf": "^0.0.28",
    "@types/stack-trace": "^0.0.28",
    "abstracted-admin": "^0.12.3",
    "abstracted-client": "^0.12.3",
    "abstracted-firebase": "^0.12.9",
    "async-shelljs": "^0.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "chance": "^1.0.16",
    "coveralls": "^3.0.0",
    "faker": "^4.1.0",
    "firemock": "^0.18.0",
    "js-yaml": "^3.12.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.0",
    "rollup": "^0.60.1",
    "rollup-plugin-typescript2": "^0.15.0",
    "test-console": "^1.1.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.0.0-dev.20180605",
    "vuepress": "^0.10.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}

{
  "name": "firemodel",
  "version": "0.17.1",
  "description": "Modeling, Access, and Mocking for Firebase Projects",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/firemodel",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "scripts": {
    "clean": "rimraf lib;",
    "clean:nuclear": "rimraf lib; rimraf node_modeles; yarn cache clean; rm yarn.lock; yarn ",
    "lint": "./node_modules/.bin/tslint --force --format verbose \"src/**/*.ts\"",
    "build": "yarn build:tsc",
    "build:rollup": "tslint src/**/*.ts && rollup -c rollup.config.js",
    "build:tsc": "tslint src/**/*.ts && tsc -P tsconfig.cjs.json && tsc -P tsconfig.esnext.json",
    "test": "./node_modules/.bin/ts-node ./scripts/test.ts ",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "./node_modules/.bin/ts-node ./scripts/watch.ts",
    "watch:test": "./node_modules/.bin/ts-node ./scripts/watch.ts test",
    "watch:all": "./node_modules/.bin/ts-node ./scripts/watch.ts all",
    "deploy": "./node_modules/.bin/ts-node ./scripts/deploy.ts",
    "package": "./node_modules/.bin/ts-node ./scripts/package.ts ",
    "cli": "./node_modules/.bin/ts-node cli/index.ts --",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "abstracted-firebase": "^0.14.5",
    "common-types": "^1.9.6",
    "firebase-api-surface": "^0.8.5",
    "firebase-key": "^2.0.1",
    "lodash": "^4.17.11",
    "pluralize": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "serialized-query": "^0.4.5",
    "typed-conversions": "^0.8.13",
    "wait-in-parallel": "^1.2.0"
  },
  "peerDependencies": {
    "firemock": "^0.18.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chance": "^0.7.36",
    "@types/faker": "^4.1.0",
    "@types/js-yaml": "^3.9.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.2",
    "@types/node": "^9.6.2",
    "@types/pluralize": "^0.0.28",
    "@types/rimraf": "^2.0.2",
    "@types/stack-trace": "^0.0.28",
    "@vuepress/plugin-back-to-top": "^1.0.0-alpha.0",
    "@vuepress/plugin-last-updated": "^1.0.0-alpha.0",
    "@vuepress/plugin-medium-zoom": "^1.0.0-alpha.0",
    "@vuepress/plugin-pwa": "^1.0.0-alpha.0",
    "abstracted-admin": "^0.14.10",
    "abstracted-client": "^0.14.16",
    "async-shelljs": "^0.1.2",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "chance": "^1.0.18",
    "coveralls": "^3.0.3",
    "faker": "^4.1.0",
    "firemock": "^0.18.3",
    "js-yaml": "^3.12.1",
    "mocha": "^6.0.1",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.0",
    "rollup": "^0.62.0",
    "rollup-plugin-typescript2": "^0.15.1",
    "test-console": "^1.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.3.3",
    "vuepress": "^1.0.0-alpha.39"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}

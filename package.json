    "@vuepress/plugin-medium-zoom": "^1.4.1",
    "@vuepress/plugin-pwa": "^1.4.1",
    "async-shelljs": "^0.1.4",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "do-devops": "^0.3.6",
    "fake-indexeddb": "^3.0.0",
    "faker": "^4.1.0",

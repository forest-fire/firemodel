{
  "name": "firemodel",
  "version": "0.52.6",
  "description": "Modeling, Access, and Mocking for Firebase Projects",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/firemodel",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/es/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "clean:nuclear": "rimraf lib dist node_modules && yarn cache clean && rm yarn.lock && yarn && yarn upgrade",
    "lint": "./node_modules/.bin/tslint --force --format verbose \"src/**/*.ts\"",
    "build": "run-s clean build:tsc build:cjs",
    "build:rollup": "rollup -c",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "build:tsc": "yarn tslint src/**/*.ts && yarn tsc -P tsconfig.es.json && yarn tsc -P tsconfig.cjs.json",
    "build:nuclear": "yarn clean:nuclear && yarn build",
    "test": "./node_modules/.bin/ts-node ./scripts/test.ts ",
    "deploy": "yarn build:nuclear && yarn test && yarn publish",
    "package": "./node_modules/.bin/ts-node ./scripts/package.ts ",
    "cli": "./node_modules/.bin/ts-node cli/index.ts --",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "watch": "run-p watch:es watch:cjs",
    "watch:es": "tsc -w -P tsconfig.esnext.json",
    "watch:cjs": "tsc -w -P tsconfig.cjs.json",
    "watch:autoindex": "do autoindex",
    "visualize": "yarn madge src --extensions ts -i /tmp/dep-visualize.png && open /tmp/dep-visualize.png",
    "circular": "yarn madge src --extensions ts --circular",
    "orphans": "yarn madge src --extensions ts --orphans",
    "leaves": "yarn madge src --extensions ts --leaves",
    "warnings": "yarn madge src --extensions ts --warnings",
    "summary": "yarn madge src --extensions ts --summary"
  },
  "dependencies": {
    "common-types": "^1.15.6",
    "date-fns": "^2.14.0",
    "dexie": "^3.0.1",
    "fast-copy": "2.0.5",
    "fast-deep-equal": "^3.1.3",
    "firebase-key": "^2.0.2",
    "firemock": "^0.52.7",
    "get-value": "^3.0.1",
    "lodash.first": "^3.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "set-value": "^3.0.2",
    "typed-conversions": "^0.10.2",
    "universal-fire": "^0.52.7",
    "wait-in-parallel": "^1.2.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/chai": "^4.2.9",
    "@types/faker": "^4.1.12",
    "@types/get-value": "^3.0.1",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@types/lodash.first": "^3.0.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^10.14.0",
    "@types/pluralize": "^0.0.29",
    "@types/rimraf": "^2.0.3",
    "@types/set-value": "^2.0.0",
    "@types/stack-trace": "^0.0.29",
    "@vuepress/plugin-back-to-top": "^1.5.0",
    "@vuepress/plugin-last-updated": "^1.5.0",
    "@vuepress/plugin-medium-zoom": "^1.5.0",
    "@vuepress/plugin-pwa": "^1.5.0",
    "async-shelljs": "^0.1.4",
    "babel-jest": "^26.0.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "do-devops": "^0.3.18",
    "fake-indexeddb": "^3.0.2",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "js-yaml": "^3.13.1",
    "madge": "^3.9.2",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "rollup": "^2.17.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "test-console": "^1.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "vue-property-decorator": "^8.4.2",
    "vuepress": "^1.5.0",
    "vuepress-plugin-autometa": "^0.1.13"
  },
  "engines": {
    "node": ">=8.6.0"
  }
}
{
  "name": "firemodel",
  "version": "0.48.4",
  "description": "Modeling, Access, and Mocking for Firebase Projects",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/firemodel",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf lib;",
    "clean:nuclear": "rimraf lib dist node_modules && yarn cache clean && rm yarn.lock && yarn && yarn upgrade",
    "lint": "./node_modules/.bin/tslint --force --format verbose \"src/**/*.ts\"",
    "build": "yarn tslint src/**/*.ts && yarn tsc -P tsconfig.esnext.json && yarn tsc -P tsconfig.cjs.json",
    "build:nuclear": "yarn clean:nuclear && yarn build",
    "test": "./node_modules/.bin/ts-node ./scripts/test.ts ",
    "deploy": "yarn build:nuclear && yarn test && yarn publish",
    "publish": "yarn build",
    "package": "./node_modules/.bin/ts-node ./scripts/package.ts ",
    "cli": "./node_modules/.bin/ts-node cli/index.ts --",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "circular": "yarn madge src/**/*.ts --circular",
    "watch": "run-p watch:es watch:cjs",
    "watch:es": "tsc -w -P tsconfig.esnext.json",
    "watch:cjs": "tsc -w -P tsconfig.cjs.json",
    "watch:autoindex": "do autoindex"
  },
  "dependencies": {
    "@forest-fire/real-time-db": "^0.48.14",
    "@forest-fire/serialized-query": "^0.48.14",
    "common-types": "^1.14.3",
    "date-fns": "^2.14.0",
    "dexie": "^3.0.1",
    "fast-copy": "2.0.5",
    "fast-deep-equal": "^3.1.1",
    "firebase-key": "^2.0.2",
    "firemock": "^0.48.14",
    "get-value": "^3.0.1",
    "lodash.first": "^3.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "set-value": "^3.0.0",
    "typed-conversions": "^0.10.2",
    "universal-fire": "^0.48.14",
    "wait-in-parallel": "^1.2.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/faker": "^4.1.12",
    "@types/get-value": "^3.0.1",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@types/lodash.first": "^3.0.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^10.14.0",
    "@types/pluralize": "^0.0.29",
    "@types/rimraf": "^2.0.3",
    "@types/set-value": "^2.0.0",
    "@types/stack-trace": "^0.0.29",
    "@vuepress/plugin-back-to-top": "^1.5.0",
    "@vuepress/plugin-last-updated": "^1.5.0",
    "@vuepress/plugin-medium-zoom": "^1.5.0",
    "@vuepress/plugin-pwa": "^1.5.0",
    "async-shelljs": "^0.1.4",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "do-devops": "^0.3.6",
    "fake-indexeddb": "^3.0.0",
    "faker": "^4.1.0",
    "js-yaml": "^3.13.1",
    "madge": "^3.9.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "test-console": "^1.1.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3",
    "vue-property-decorator": "^8.4.2",
    "vuepress": "^1.5.0",
    "vuepress-plugin-autometa": "^0.1.13"
  },
  "engines": {
    "node": ">=8.6.0"
  }
}
